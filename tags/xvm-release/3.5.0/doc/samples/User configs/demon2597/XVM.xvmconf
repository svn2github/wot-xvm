        "medium": "\u003B",
        "heavy": "\u003F",
        "td": "\u002E",
        "spg": "\u002D",
        "superh": "" // superHeavyTank доп класс посредством gui_settings.xml, кто пользуется- вставить в кавычки значение \u004A
