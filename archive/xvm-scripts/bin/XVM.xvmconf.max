            }
          ]
        }
      },
      "dead": {
        "normal": {
          "vehicleIcon": {
            "maxScale": 80
          },
          "damageText": {
            "y": -65,
            "damageMessage": "<textformat leading='-5'>({{dmg}})<br>{{vehicle}}</textformat>",
            "blowupMessage": "<textformat leading='-5'>Blow-up!<br>{{vehicle}}</textformat>"
          },
