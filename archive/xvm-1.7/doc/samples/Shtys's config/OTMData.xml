<?xml version="1.0" encoding="utf-8"?>
<overTargetMarkers>
  <battle>
    <drawGrid>false</drawGrid>
  </battle>
  <rating>
    <showPlayersStatistics>false</showPlayersStatistics>
  </rating>
  <components>
    <friend>
      <combatScrollText>
        <enabled value="true"/>
        <speed value="2"/>
	<color value=""/>
        <maxRange value="40"/>
        <textSize value="15"/>
        <message>Боеукладка!</message>
        <prefix>-</prefix>
        <postfix></postfix>
        <filter type="DropShadowFilter" size="3" angle="45" distance="0" color="0x000000" alpha="100" strength="150"/>
        <hitKind>Absolute</hitKind>
        <font>Cuprum</font>
      </combatScrollText>      
      <healthBar>
        <border size="1" color="0x000000" alpha="30"/>
        <fill width="60" height="2" color="" alpha="80"/>
        <damage fade="0" color="" alpha="100"/>
      </healthBar>
      <vehicleIcon>
        <fill color=""/>
        <scale maxScale="100" x="0" y="16"/>
      </vehicleIcon>
      <infoText>
        <format size="15" align="center" font="Cuprum" width="200"/>
        <filter type="DropShadowFilter" size="3" angle="45" distance="0" color="0x000000" alpha="100" strength="150"/>
      </infoText>
    </friend>
    <enemy>
      <combatScrollText>
        <enabled value="true"/>
        <speed value="2"/>
	<color value=""/>
        <maxRange value="40"/>
        <textSize value="15"/>
        <message>Боеукладка!</message>
        <prefix>-</prefix>
        <postfix></postfix>
        <filter type="DropShadowFilter" size="3" angle="45" distance="0" color="0x000000" alpha="100" strength="150"/>
        <hitKind>Absolute</hitKind>
        <font>Cuprum</font>
      </combatScrollText> 
      <healthBar>
        <border size="1" color="0x000000" alpha="30"/>
        <fill width="60" height="2" color="0xEC3737" alpha="80"/>
        <damage fade="0" color="" alpha="100"/>
      </healthBar>
      <vehicleIcon>
        <fill color=""/>
        <scale maxScale="100" x="0" y="16"/>
      </vehicleIcon>      
      <infoText>
        <format size="15" align="center" font="Cuprum" width="200"/>
        <filter type="DropShadowFilter" size="3" angle="45" distance="0" color="0x000000" alpha="100" strength="150"/>
      </infoText>
    </enemy>
  </components>
  <behavior>
    <friend>
      <alive>
        <normal>
          <actionMarker x="0" y="-65"/>
          <combatScrollText x="0" y="-75"/>
          <healthBar x="-31" y="-38" alpha="100" visible="true"/>
          <vehicleIcon x="0" y="-15" alpha="100" visible="true"/>
          <infoText x="0" y="-20" color="" alpha="100" visible="true" bold="true" format="{{hp}}"/>
          <infoText x="0" y="-42" color="" alpha="100" visible="true" bold="true" format="{{vehicle}}"/>
        </normal>
        <extended>
          <actionMarker x="0" y="-65"/>
          <combatScrollText x="0" y="-75"/>
