          <vehicleIcon x="0" y="-15" alpha="100" visible="true"/>
          <infoText x="0" y="-20" color="" alpha="100" visible="false" format="{{hp}} ({{hp-ratio}}%)"/>
        </normal>
        <extended>
          <actionMarker x="0" y="-65"/>
          <combatScrollText x="0" y="-75"/>
