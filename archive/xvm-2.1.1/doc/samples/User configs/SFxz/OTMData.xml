        <speed value="2"/>
        <maxRange value="40"/>
        <textSize value="14"/>
        <message>Blow-up!</message>
        <prefix>-</prefix>
        <postfix/>
        <filter type="DropShadowFilter" size="1" angle="45" distance="1" color="0x000000" alpha="100" strength="200"/>
        <hitKind>Absolute</hitKind>
        <font>$TextFont</font>
	  </combatScrollText>
      <contourIcon>
        <tint amount="0" color="0x00FF00"/>
      </contourIcon>
      <currentHealth>
        <format size="11" align="center" font="$FieldFont" bold="true"/>
