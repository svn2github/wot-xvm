        <maxValue enabled="true"/>
      </currentHealth>
      <healthBar>
        <border size="1" color="0x000000" alpha="30"/>
        <fill width="80" height="12" alpha="30"/>
        <damage fade="1" alpha="65"/>
      </healthBar>
      <healthRatio>
