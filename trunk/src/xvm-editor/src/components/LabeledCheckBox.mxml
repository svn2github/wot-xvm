<?xml version="1.0" encoding="utf-8"?>
<c:LabeledComponent xmlns:fx="http://ns.adobe.com/mxml/2009"
	    xmlns:s="library://ns.adobe.com/flex/spark"
	    xmlns:mx="library://ns.adobe.com/flex/mx"
		xmlns:c="components.*"
		width="100%"
		height="20"
		borderVisible="false">
	<fx:Script>
		// value
		[Bindable]
		public override function get value():*
		{
			return edit.selected;
		}
		public override function set value(value:*): void
		{
			edit.selected = value;
		}
	</fx:Script>
	<s:CheckBox left="0" right="0" label="{label}" width="{width}" change="onChange(event)" id="edit"/>
</c:LabeledComponent>
