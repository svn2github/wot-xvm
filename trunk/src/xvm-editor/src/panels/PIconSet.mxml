<?xml version="1.0" encoding="utf-8"?>
<c:PanelComponent
	xmlns:fx="http://ns.adobe.com/mxml/2009"
	xmlns:s="library://ns.adobe.com/flex/spark"
	xmlns:mx="library://ns.adobe.com/flex/mx"
	xmlns:c="components.*"
	width="100%" height="210" title="Пути к наборам иконок (относительно res_mods/&lt;ver&gt;/gui/flash/)">
	<mx:Image right="5" y="-18" width="16" height="16" source="@Embed(source='cancel.png')" toolTip="Вернуть настройки по умолчанию" useHandCursor="true" buttonMode="true"/>
	<s:VGroup left="10" right="10" top="10" bottom="10" gap="2" variableRowHeight="true">
		<c:LabeledTextInput label="Путь к иконкам для экрана загрузки боя" labelWidth="100%" wrapTextField="true" height="40" id="c_iconset_battleLoading" change="onChange(event)"/>
		<c:LabeledTextInput label="Путь к иконкам для статистики боя (по клавише Tab)" labelWidth="100%" wrapTextField="true" height="40" id="c_iconset_statisticForm" change="onChange(event)"/>
		<c:LabeledTextInput label="Путь к иконкам для панелей игроков (ушей)" labelWidth="100%" wrapTextField="true" height="40" id="c_iconset_playersPanel" change="onChange(event)"/>
		<c:LabeledTextInput label="Путь к иконкам для маркеров над танками" labelWidth="100%" wrapTextField="true" height="40" id="c_iconset_vehicleMarker" change="onChange(event)"/>
	</s:VGroup>
</c:PanelComponent>
