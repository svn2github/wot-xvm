<?xml version="1.0" encoding="utf-8"?>
<c:PanelComponent
    xmlns:fx="http://ns.adobe.com/mxml/2009"
    xmlns:s="library://ns.adobe.com/flex/spark"
    xmlns:mx="library://ns.adobe.com/flex/mx"
    xmlns:c="components.*"
    height="100%"
    title="{resourceManager.getString('panels', 'PIconSet')}">
    <c:LabeledTextInput label="{resourceManager.getString('panels', 'iconset_battleLoading')}" labelWidth="100%" wrapTextField="true" height="40" id="v_battleLoading" config="iconset.battleLoading" change="onChange(event)"/>
    <c:LabeledTextInput label="{resourceManager.getString('panels', 'iconset_statisticForm')}" labelWidth="100%" wrapTextField="true" height="40" id="v_statisticForm" config="iconset.statisticForm" change="onChange(event)"/>
    <c:LabeledTextInput label="{resourceManager.getString('panels', 'iconset_playersPanel')}" labelWidth="100%" wrapTextField="true" height="40" id="v_playersPanel" config="iconset.playersPanel" change="onChange(event)"/>
    <c:LabeledTextInput label="{resourceManager.getString('panels', 'iconset_vehicleMarker')}" labelWidth="100%" wrapTextField="true" height="40" id="v_vehicleMarker" config="iconset.vehicleMarker" change="onChange(event)"/>
</c:PanelComponent>
