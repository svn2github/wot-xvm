<?xml version="1.0" encoding="utf-8"?>
<c:PanelComponent
	xmlns:fx="http://ns.adobe.com/mxml/2009"
	xmlns:s="library://ns.adobe.com/flex/spark"
	xmlns:mx="library://ns.adobe.com/flex/mx"
	xmlns:c="components.*"
	width="100%" height="210" title="{resourceManager.getString('panels', 'PIconSet')}">
	<fx:Metadata>
		[ResourceBundle("panels")]
	</fx:Metadata>
	<mx:Image right="5" y="-18" width="16" height="16" source="@Embed(source='images/cancel.png')" toolTip="{resourceManager.getString('panels', 'DefaultSettings')}" useHandCursor="true" buttonMode="true"/>
	<s:VGroup left="10" right="10" top="10" bottom="10" gap="2" variableRowHeight="true">
		<c:LabeledPathSelector label="{resourceManager.getString('panels', 'iconset_battleLoading')}" labelWidth="100%" wrapTextField="true" height="40" id="v_battleLoading" config="iconset.battleLoading" change="onChange(event)"/>
		<c:LabeledPathSelector label="{resourceManager.getString('panels', 'iconset_statisticForm')}" labelWidth="100%" wrapTextField="true" height="40" id="v_statisticForm" config="iconset.statisticForm" change="onChange(event)"/>
		<c:LabeledPathSelector label="{resourceManager.getString('panels', 'iconset_playersPanel')}" labelWidth="100%" wrapTextField="true" height="40" id="v_playersPanel" config="iconset.playersPanel" change="onChange(event)"/>
		<c:LabeledPathSelector label="{resourceManager.getString('panels', 'iconset_vehicleMarker')}" labelWidth="100%" wrapTextField="true" height="40" id="v_vehicleMarker" config="iconset.vehicleMarker" change="onChange(event)"/>
	</s:VGroup>
</c:PanelComponent>
