<?xml version="1.0" encoding="utf-8"?>
<c:PanelComponent
    xmlns:fx="http://ns.adobe.com/mxml/2009"
    xmlns:s="library://ns.adobe.com/flex/spark"
    xmlns:mx="library://ns.adobe.com/flex/mx"
    xmlns:c="components.*"
    height="100%"
    title="{_('PRating')}">
    <c:LabeledCheckBox label="{_('showPlayersStatistics')}" id="v_showPlayersStatistics" config="rating.showPlayersStatistics" change="onChange(event)"/>
    <c:LabeledCheckBox label="{_('enableUserInfoStatistics')}" id="v_enableUserInfoStatistics" config="rating.enableUserInfoStatistics" change="onChange(event)"/>
    <c:LabeledCheckBox label="{_('enableCompanyStatistics')}" id="v_enableCompanyStatistics" config="rating.enableCompanyStatistics" change="onChange(event)"/>
    <c:LabeledCheckBox label="{_('loadEnemyStatsInFogOfWar')}" id="v_loadEnemyStatsInFogOfWar" config="rating.loadEnemyStatsInFogOfWar" change="onChange(event)"/>
    <c:LabeledCheckBox label="{_('enableStatisticsLog')}" id="v_enableStatisticsLog" config="rating.enableStatisticsLog" change="onChange(event)"/>
</c:PanelComponent>
