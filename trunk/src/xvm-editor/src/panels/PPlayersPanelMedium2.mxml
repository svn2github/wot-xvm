<?xml version="1.0" encoding="utf-8"?>
<c:PanelComponent
    xmlns:fx="http://ns.adobe.com/mxml/2009"
    xmlns:s="library://ns.adobe.com/flex/spark"
    xmlns:mx="library://ns.adobe.com/flex/mx"
    xmlns:c="components.*"
    title="{resourceManager.getString('panels', 'PPlayersPanelMedium2')}">
    <c:LabeledNumberSelector label="{resourceManager.getString('panels', 'VehicleNameWidth')}" id="v_width" config="playersPanel.medium2.width" minimum="0" maximum="250" change="onChange(event)"/>
    <c:LabeledTextInput label="{resourceManager.getString('panels', 'FormatVehicleNameLeft')}" labelWidth="100%" wrapTextField="true" height="40" id="v_formatLeft" config="playersPanel.medium2.formatLeft" change="onChange(event)"/>
    <c:LabeledTextInput label="{resourceManager.getString('panels', 'FormatVehicleNameRight')}" labelWidth="100%" wrapTextField="true" height="40" id="v_formatRight" config="playersPanel.medium2.formatRight" change="onChange(event)"/>
</c:PanelComponent>
