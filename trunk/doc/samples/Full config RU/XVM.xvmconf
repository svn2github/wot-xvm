          "oneself": {"x": 0, "y": 0}   // Смещение поля относительно текущей иконки.
        },
        "alpha" : { // Прозрачность текстовых полей.
          "ally":  100,
          "enemy": 100,
          "squad": 100,
          "lost":  70,
          "oneself": 100
        }
      },
      "mapSize": { // Поле размера стороны карты. Например 1000м, 700м, 600м.
