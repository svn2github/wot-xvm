  "turretMarkers": {
    "highVulnerability": "*", // Субъект имеет стоковую башню и топ орудие не может быть установлено. Субъект сильно уязвим.
    "lowVulnerability": "'"   // Субъект имеет стоковую башню и топ орудие может быть установлено. Субъект немного уязвим.
  },
  // Лог попаданий (счетчик своих попаданий)
  "hitLog": {
    "visible": true,      // переключатель вкл/выкл
    "x": 202,             // Позиция X (отрицательные значения - привязать к правой стороне экрана)
    "y": 25,               // Позиция Y (отрицательные значения - привязать к нижней стороне экрана)
    "w": 500,             // Ширина
    "h": 200,            // Высота
    "lines": 7,           // Количество строк. Устаревшие данные выталкиваются.
