    "removeSquadIcon": false, // true - убрать отображение иконки взвода
    // параметры отображения иконки игрока/клана (см. battleLoading выше).
    "clanIcon": { "show": true, "x": 82, "y": 6, "xr": 82, "yr": 6, "h": 16, "w": 16, "alpha": 90 },
    // режим ушей "medium" - первые средние уши в игре
    "medium": {
      "width": 80,    // 0..250 - задает ширину поля имени игрока. По умолчанию: 46
