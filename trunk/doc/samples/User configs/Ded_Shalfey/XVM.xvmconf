      "formatLeft": "<font color='{{c:t-rating}}'>{{vehicle}}</font>",
      // формат отображения для правой панели (допускаются макроподстановки, см. readme-ru.txt)
      "formatRight": "<font color='{{c:t-rating}}'>{{vehicle}}</font>"
