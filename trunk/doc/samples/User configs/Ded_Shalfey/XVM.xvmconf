    "clanIcon": { "show": true, "x": 81, "y": 6, "xr": 81, "yr": 6, "h": 16, "w": 16, "alpha": 90 },
    // формат отображения для левой панели (допускаются макроподстановки, см. readme-ru.txt)
    "formatLeft": "<font face='Comic Sans MS' size='11'><font color='{{c:kb}}'>{{kb}}</font> <font color='{{c:eff}}'>{{eff}}</font> <font color='{{c:rating}}'>{{rating}}</font></font> | <font face='Comic Sans MS' size='11'><font color='{{c:t-battles}}'>{{t-battles:4}}</font> <font color='{{c:t-rating}}'>{{t-rating:3}}</font></font>",
    // формат отображения для правой панели (допускаются макроподстановки, см. readme-ru.txt)
    "formatRight": "<font face='Comic Sans MS' size='11'><font color='{{c:t-battles}}'>{{t-battles:4}}</font> <font color='{{c:t-rating}}'>{{t-rating:3}}</font></font> | <font face='Comic Sans MS' size='11'><font color='{{c:rating}}'>{{rating}}</font> <font color='{{c:eff}}'>{{eff}}</font> <font color='{{c:kb}}'>{{kb}}</font></font> "
  },
  // Параметры панелей игроков ("ушей")
  "playersPanel": {
    "alpha": 80,       // 0..100 - задает прозрачность "ушей". 0 - прозрачные, 100 - не прозрачные
