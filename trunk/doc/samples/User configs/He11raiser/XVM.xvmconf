        "css": { // CSS стиль.
          "ally":  ".mm_a{font-family:Tahoma; font-size:7px; color:#C8FFA6;}",
          "enemy": ".mm_e{font-family:Tahoma; font-size:7px; color:#FCA9A4;}",
          "squad": ".mm_s{font-family:Tahoma; font-size:7px; color:#FFC099;}",
