{
  "configVersion": "1.4.0", // Версия конфига. Не удаляйте и не изменяйте её без необходимости.
  "editorVersion": "0.0.0", // Версия редактора. Пока не используется.
  // Блок общих параметров конфига. Все параметры информационные, в моде не используются.
  "definition": {
    "author": "He11raiser",                     // автор конфига
    "description": "My settings for XVM",  // описание конфига
    "url": "http://www.koreanrandom.com/forum/topic/1384-xvm-configurations/", // адрес, где выкладываеются обновления конфига
    "date": "01.03.2013",                       // дата последней модификации конфига
    "gameVersion": "0.8.0",                     // поддерживаемая версия игры
    "modMinVersion": "3.4.0"                    // минимально необходимая версия мода XVM
  },
  // Блок общих параметров боевого интерфейса
  "battle": {
    "mirroredVehicleIcons": true,               // false - отключить зеркалирования иконок танков (полезно для альтернативных иконок). По умолчанию включено.
    "showPostmortemTips": false,                // false - отключить всплывающую внизу панель после смерти. По умолчанию включено.
    "removePanelsModeSwitcher": true,           // true - убрать переключатель режимов ушей мышкой
    "drawGrid": false,                           // true - включить отображение сетки координат (может быть полезным для настройки). Не включайте, если не знаете зачем.
    "hideXVMVersion": false,                    // true - отключить отображение версии xvm в верхнем левом углу экрана
    "useStandardMarkers": false                 // true - использовать стандартные маркеры клиента
  },
  // Блок управлением статистикой (только xvm-full)
  "rating": {
    "showPlayersStatistics": true,             // true - включить отображение статистики. Глобальный переключатель - без него модуль статистики будет везде отключен.
    "loadEnemyStatsInFogOfWar": false           // true - включить загрузку данных по противникам в "тумане войны"
  },
  // Параметры экрана загрузки боя
  "battleLoading": {
    "showClock": false,                          // false - выключить отображение часов на экране загрузки боя
    "showChances": true,                       // true - включить отображение шансов на победу (только в XVM Full)
    "removeSquadIcon": false,                   // true - убрать отображение иконки взвода. На пустое поле можно вывести, например, иконку клана.
    "clanIcon": {                               // параметры отображения иконки игрока/клана
      "show": true,                             //   false - не отображать иконки игрока/клана в окне загрузки боя
      "x": 0,                                   //   положение по оси X оносительно иконки танка
      "y": 6,                                   //   положение по оси Y оносительно иконки танка
      "xr": 0,                                  //   положение по оси X для правых ушей
      "yr": 6,                                  //   положение по оси Y для правых ушей
      "w": 16,                                  //   ширина иконки игрока/клана
      "h": 16,                                  //   высота иконки игрока/клана
      "alpha": 90                               //   прозрачность иконки игрока/клана
    },
    // формат отображения для левой панели (допускаются макроподстановки, см. readme-ru.txt)
    "formatLeft": "{{vehicle}} <font color='{{c:kb}}'>{{kb}}</font> <font color='{{c:eff}}'>{{eff}}</font> <font color='{{c:rating}}'>{{rating}}</font>",
    // формат отображения для правой панели (допускаются макроподстановки, см. readme-ru.txt)
    "formatRight": "<font color='{{c:rating}}'>{{rating}}</font> <font color='{{c:eff}}'>{{eff}}</font> <font color='{{c:kb}}'>{{kb}}</font> {{vehicle}}"
  },
  // Параметры окна статистики по клавише Tab
  "statisticForm": {
    "showChances": true,     // true - включить отображение шансов на победу (только в XVM Full, пока работает только в боях 15x15)
    "removeSquadIcon": false, // true - убрать отображение иконки взвода
    // параметры отображения иконки игрока/клана (см. battleLoading выше).
   "clanIcon": { "show": true, "x": 0, "y": 6, "xr": 0, "yr": 6, "h": 16, "w": 16, "alpha": 90 },
    // формат отображения для левой панели (допускаются макроподстановки, см. readme-ru.txt)
    "formatLeft": "{{vehicle}} <font color='{{c:kb}}'>{{kb}}</font> <font color='{{c:eff}}'>{{eff}}</font> <font color='{{c:rating}}'>{{rating}}</font>",
    // формат отображения для правой панели (допускаются макроподстановки, см. readme-ru.txt)
    "formatRight": "<font color='{{c:rating}}'>{{rating}}</font> <font color='{{c:eff}}'>{{eff}}</font> <font color='{{c:kb}}'>{{kb}}</font> {{vehicle}}"
  },
  // Параметры панелей игроков ("ушей")
  "playersPanel": {
    "alpha": 60,       // 0..100 - задает прозрачность "ушей". 0 - прозрачные, 100 - не прозрачные
    "iconAlpha": 100,  // 0..100 - задает прозрачность иконок в "ушах". 0 - прозрачные, 100 - не прозрачные
    "removeSquadIcon": false, // true - убрать отображение иконки взвода
    // параметры отображения иконки игрока/клана (см. battleLoading выше).
    "clanIcon": { "show": true, "x": 0, "y": 6, "xr": 0, "yr": 6, "h": 16, "w": 16, "alpha": 90 },
    // режим ушей "medium" - первые средние уши в игре
    "medium": {
      "width": 66,    // 0..250 - задает ширину поля имени игрока. По умолчанию: 46
