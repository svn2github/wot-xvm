          "squad": {"x": 3, "y": -2},   // Field offset ralative to current icon.
          "lost":  {"x": -6, "y": -10}, // Field offset ralative to enemy last seen position.
          "oneself": {"x": 0, "y": 0}   // Field offset ralative to current icon.
        },
        "alpha" : { // Transparency
          "ally":  100,
          "enemy": 100,
          "squad": 100,
          "lost":  70,
          "oneself": 100
        }
      },
