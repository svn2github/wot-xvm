            "angle": 0,
            "alpha": 80,
            "blur": 6,
            "strength": 4
          },
          "oneself": {
            "enabled": true,
            "color": "0x000000",
            "distance": 0,
            "angle": 0,
            "alpha": 80,
            "blur": 3,
            "strength": 4
          }
        },
        "offset": {
          "ally":  {"x": 3, "y": -1},   
          "enemy": {"x": 3, "y": -1},   
          "squad": {"x": 3, "y": -2},   
          "lost":  {"x": -6, "y": -10}, 
          "oneself": {"x": 0, "y": 0}  
        },
        "alpha" : {
          "ally":  100,
          "enemy": 100,
          "squad": 100,
          "lost":  70,
          "oneself": 100
        }
     },
      
        "mapSize": { 
        "enabled": false,
        "format": "<b>{{cellsize}}0 м</b>",
        "css": "font-size:12px; color:#000000;",
        "alpha": 80,
        "offsetX": 0,
        "offsetY": 0,
        "shadow": {
          "enabled": true,
          "color": "0xAAAAAA",
          "distance": 0,
          "angle": 0,
          "alpha": 80,
          "blur": 8,
          "strength": 4
        },
        "width": 100, 
        "height": 30 
      }
    
