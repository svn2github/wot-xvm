{
  "configVersion": "1.5.0",
  "definition": {
    "author": "BOtsmAN",
    "description": "Settings for XVM",
    "url": "http://code.google.com/p/wot-xvm/",
    "date": "09.12.2012",
    "gameVersion": "0.8.4",
    "modMinVersion": "3.4.0"
  },
  "battle": {
    "mirroredVehicleIcons": true,
    "showPostmortemTips": false,
    "drawGrid": false,
    "highlightVehicleIcon": true,
    "hideXVMVersion": true,
    "clockFormat": "",     
    "useStandardMarkers": false
  },
  "rating": {
    "showPlayersStatistics": true,
    "loadEnemyStatsInFogOfWar": true,
    "enableStatisticsLog": false,              
    "enableUserInfoStatistics": true,         
    "enableCompanyStatistics": true    
  },
  "battleLoading": {
    "showChances": true,   
    "clockFormat": "H:N",     
    "clanIcon": { "show": true, "x": -348, "y": 2, "h": 20, "w": 20, "alpha": 100 },
    "formatLeft": "<font color='{{c:eff}}'>{{vehicle}}</font>  {{kb}}  {{eff}} <font color='{{c:rating}}'>{{rating}}</font>",
    "formatRight": "<font color='{{c:rating}}'>{{rating}}</font>  {{eff}}  {{kb}}  <font color='{{c:eff}}'>{{vehicle}}</font>"
  },
  "statisticForm": {
    "showChances": true, 
    "clanIcon": { "show": true, "x": -348, "y": 2, "h": 20, "w": 20, "alpha": 100 },
    "formatLeft": "<font color='{{c:eff}}'>{{vehicle}}</font>  {{kb}}  {{eff}}  <font color='{{c:rating}}'>{{rating}}</font>",
    "formatRight": "<font color='{{c:rating}}'>{{rating}}</font>  {{eff}}  {{kb}}  <font color='{{c:eff}}'>{{vehicle}}</font>"
  },
  "playersPanel": {
    "alpha": 80,
    "removeSquadIcon": true, 
    "clanIcon": { "show": false, "x": 85, "y": 4, "h": 16, "w": 16, "alpha": 100 },            
    "medium": {
     "width": 70,
      "formatLeft": "<font color='{{c:rating}}'>{{rating}}</font> <font color='{{c:eff}}'>{{vehicle}}</font>",
      "formatRight": "<font color='{{c:eff}}'>{{vehicle}}</font> <font color='{{c:rating}}'>{{rating}}</font>"
    },
    "medium2": {
      "formatLeft": "<font color='{{c:eff}}'>{{vehicle}}</font>",
      "formatRight": "<font color='{{c:eff}}'>{{vehicle}}</font>"
    },
    "large": {
      "width": 130,
      "nickFormatLeft": "<font color='{{c:rating}}'>{{rating}}</font> {{nick}}",
      "nickFormatRight": "{{nick}} <font color='{{c:rating}}'>{{rating}}</font>",
      "vehicleFormatLeft": "<font color='{{c:eff}}'>{{vehicle}}</font>",
      "vehicleFormatRight": "<font color='{{c:eff}}'>{{vehicle}}</font>"
    }
  },
  "finalStatistic": {
    "startPage": 1,           // номер начальной страницы (1, 2, 3)
    // 1 - взвод
    // 2 - имя игрока
    // 3 - модель танка
    // 4 - урон нанесенный за бой
    // 5 - фраги за бой
    // 6 - опыт за бой
    // 7 - медали за бой
    "sortColumn": 4,          // номер колонки для сортировки (1 - 7)
    "showChances": false,    
    "showChancesExp": false  
  },
  "turretMarkers": {
    "highVulnerability": "*", 
    "lowVulnerability": "'"  
  },
  "hitLog": {
    "visible": true,     
    "x": 240,
    "y": 8,
    "w": 250,
    "h": 20,
    "lines": 1,
    "direction": "down",
    "insertOrder": "begin", 
    "groupHitsByPlayer": false, 
    "deadMarker": "\u0029",
    "blowupMarker": "\u0028",
    "formatHeader": "<font color='0xFFFFFF'>Hits:</font> {{n}}  <font color='0xFFFFFF'>Total:</font> <b><font color='#FFCC66'>{{dmg-total}}</font></b>  <font color='0xFFFFFF'>Last:</font> {{dmg}}",
    "formatHistory": "<textformat leading='0' tabstops='[20,55,90,130,175]'><font color='#CCCCCC' size='13'>\u00D7{{n-player}}<tab>{{dmg-kind}}<tab>-{{dmg}}<tab>({{dmg-player}})<tab><font face='XVM Symbol'><font color='#FFD072'><font size='19'><font size='1{{dead}}'>{{dead}}</font></font></font><font size='1'><font size='19{{dead}}'>\u002A</font></font></font><tab>{{vehicle}}</font></textformat>"
  },
  "captureBar": {
    "enabled": true, 
    "primaryTitleOffset": 7, 
    "enemy": { 
      "primaryTitleFormat":   "<font size='15' color='#FFFFFF'>Захват базы врага!  {{extra}}</font>", 
      "secondaryTitleFormat": "<font size='15' color='#FFFFFF'>{{points}}</font>",              
      "captureDoneFormat":    "<font size='17' color='#FFCC66'>Вражеская база захвачена!</font>",    
      "extra": "Захватчиков: <b><font color='#FFCC66'>{{tanks}}</font></b>  Осталось: <b><font color='#FFCC66'>{{time}}</font><b>",                          
      "shadow": {          
        "color": "0x000000", 
        "alpha": 50,       
        "blur": 4,        
        "strength": 2      
      }
    },
    "ally": { 
      "primaryTitleFormat":   "<font size='15' color='#FFFFFF'>Захват нашей базы!  {{extra}}</font>",
      "secondaryTitleFormat": "<font size='15' color='#FFFFFF'>{{points}}</font>",
      "captureDoneFormat":    "<font size='17' color='#FFCC66'>Наша база захвачена!</font>",
      "extra": "Захватчиков: <b><font color='#FFCC66'>{{tanks}}</font></b>  Осталось: <b><font color='#FFCC66'>{{time}}</font><b>",
      "shadow": {
        "color": "0x000000",
        "alpha": 50,
        "blur": 4,
        "strength": 2
      }
    }
  },
  "minimap": {
    "enabled": true, 

    "mapBackgroundImageAlpha": 100, 
    "selfIconAlpha": 100,           

    "iconScale": 0.6,                                                   
    "labels": {                     
      "nickShrink": 5,              
      "vehicleclassmacro": {       
        "light": "\u2022",
        "medium": "\u2022",
        "heavy": "\u2022",
        "td": "\u2022",
        "spg": "\u2022",
        "superh": "\u2022"
      },
      "units": { 
       "revealedEnabled": true, 
        "lostEnemyEnabled": true,
        "format": {          
          "ally":  "<span class='mm_a'>{{vehicle}}</span>",
          "enemy": "<span class='mm_e'>{{vehicle}}</span>",
          "squad": "<textformat leading='-1'><span class='mm_s'><i>{{short-nick}}</i></span><textformat>", 
          "lost":  "<span class='mm_lclass'>{{vehicle-class}}</span><span class='mm_l'><i>{{vehicle}}</i></span>",
          "oneself": ""
       },
        "css": { 
          "ally":  ".mm_a{font-family:$FieldFont; font-size:6px; color:#C8FFA6;}",
          "enemy": ".mm_e{font-family:$FieldFont; font-size:6px; color:#FCA9A4;}",
          "squad": ".mm_s{font-family:$FieldFont; font-size:6px; color:#FFD0B2;}",
          "lost":  ".mm_l{font-family:$FieldFont; font-size:6px; color:#FCA9A4;} .mm_lclass{font-family:Arial; font-size:17px; color:#FCA9A4;}",
          "oneself": ".mm_o{font-family:$FieldFont; font-size:6px; color:#FFFFFF;}"
      },
        "shadow": {
          "ally": {
            "enabled": true,
            "color": "0x000000",
            "distance": 0,
            "angle": 0,
            "alpha": 80,
            "blur": 3,
            "strength": 4
          },
          "enemy": {
