      <infoText>
        <format size="16" align="center" font="$FieldFont"/>
        <filter type="DropShadowFilter" size="3" angle="0" distance="1" color="0x000000" alpha="100" strength="120"/>
        <maxValue enabled="true"/>
      </infoText>
      <playerName>
        <format size="14" align="center" font="$FieldFont"/>
        <filter type="DropShadowFilter" size="1" angle="45" distance="1" color="0x000000" alpha="100" strength="120"/>
      </playerName>
      <vehicleIcon>
        <fill color="16777205"/>
        <scale maxScale="110" x="0" y="16"/>
      </vehicleIcon>
      <vehicleName>
        <format size="13" align="center" font="$FieldFont"/>
        <filter type="DropShadowFilter" size="1" angle="45" distance="1" color="0x000000" alpha="100" strength="120"/>
      </vehicleName>
    </friend>
