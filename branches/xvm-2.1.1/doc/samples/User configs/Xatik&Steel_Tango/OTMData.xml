        <enabled value="true"/>
        <speed value="13"/>
	<color value="14475223"/>
        <maxRange value="400"/>
        <textSize value="14"/>
        <message>Подставился!</message>
        <prefix>-</prefix>
        <postfix/>
        <filter type="DropShadowFilter" size="4" angle="45" distance="2" color="0x000000" alpha="100" strength="200"/>
        <hitKind>Absolute</hitKind>
        <font>$TextFont</font>
      </combatScrollText>
      <contourIcon>
        <tint amount="0" color="0x00FF00"/>
      </contourIcon>
      <currentHealth>
        <format size="12" align="center" font="$FieldFont"/>
        <filter type="DropShadowFilter" size="5" angle="45" distance="2" color="0x000000" alpha="100" strength="60"/>
        <maxValue enabled="true"/>
      </currentHealth>
      <healthBar>
        <border size="1" color="0x459780" alpha="60"/>
        <fill width="50" height="2" color="16646142" alpha="85"/>
        <damage fade="1" color="16595456" alpha="100"/>
      </healthBar>
      <healthRatio>
        <format size="11" align="center" font="$FieldFont"/>
        <filter type="DropShadowFilter" size="5" angle="45" distance="2" color="0x197125" alpha="100" strength="100"/>
